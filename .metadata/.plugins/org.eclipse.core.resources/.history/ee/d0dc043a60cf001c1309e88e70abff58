import java.util.Scanner;


public class Nagative {

	public static void main(String[] args) {
		Scanner stdIn = new Scanner(System.in);
		System.out.println("정수값:");
		int n = stdIn.nextInt();
		
		if(n>0) {
			System.out.println("양수 입니다.");
		}
		else {
			System.out.println("이 값은 음의 값입니다.");
		}
		

	}

}
