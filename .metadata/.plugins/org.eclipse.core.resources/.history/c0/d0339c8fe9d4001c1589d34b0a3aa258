package test;

import java.util.Scanner;
public class munja {

	public static void main(String[] args) {
		int num[] = {1,2,3,4,5};
		
		String myName[] = {"aaa" , "bbb" , "ccc" , "ddd" , "eee"};
		
		String adr[] = {"안산" , "부산" , "안양" , "인천" , "서울"};
		
		for(int i=0; i<5; i++) {
			System.out.println(num[i] + 't' + myName[i] + 't' + adr[i] );
		}
		
	
		
		

	}

}
